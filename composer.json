{
  "name": "phalcon/docs",
  "description": "Official Phalcon Documentation",
  "keywords": [
    "documentation",
    "phalcon",
    "framework",
    "extension"
  ],
  "type": "project",
  "license": "BSD-3-Clause",
  "homepage": "https://docs.phalconphp.com/",
  "authors": [
    {
      "name": "Phalcon Team",
      "email": "team@phalconphp.com",
      "homepage": "https://phalconphp.com/en/team"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/phalcon/cphalcon/graphs/contributors"
    }
  ],
  "support": {
    "source": "https://github.com/phalcon/cphalcon",
    "issues": "https://github.com/phalcon/cphalcon/issues",
    "wiki": "https://github.com/phalcon/cphalcon/wiki",
    "forum": "https://forum.phalconphp.com/",
    "docs": "https://docs.phalconphp.com"
  },
  "autoload": {
    "psr-4": {
      "Docs\\": "app/library",
      "Docs\\Cli\\Tasks\\": "app/tasks/",
      "Docs\\Controllers\\": "app/controllers/"
    }
  },
  "require": {
    "php": ">= 5.6",
    "vlucas/phpdotenv": "^2.4",
    "dariuszp/cli-progress-bar": "^1.0",
    "symfony/options-resolver": "^2.8.1",
    "symfony/console":"^2.8.1",
    "erusev/parsedown": "^1.6",
    "erusev/parsedown-extra": "^0.7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "codeception/codeception": "^2.2",
    "codeception/mockery-module": "^0.2",
    "squizlabs/php_codesniffer": "~2.7"
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}
